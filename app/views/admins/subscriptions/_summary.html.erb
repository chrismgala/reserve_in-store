<div class="row">
  <div class="col-lg-6">
    <h5>Subscription</h5>
    <table class="table table-responsive table-striped">
      <tr>
        <th>Plan id</th>
        <td><%= subscription.plan_attributes['id'] %></td>
      </tr>
      <tr>
        <th>Plan code</th>
        <td><%= subscription.plan_attributes['code'] %></td>
      </tr>
      <tr>
        <th>Name</th>
        <td><%= subscription.plan_attributes['name'] %></td>
      </tr>
      <tr>
        <th>Created At</th>
        <td>
          <%= local_time(subscription.created_at) %>
          (<%= time_ago_in_words(subscription.created_at) %> ago)
        </td>
      </tr>
      <tr>
        <th>Updated At</th>
        <td>
          <%= local_time(subscription.updated_at) %>
          (<%= time_ago_in_words(subscription.updated_at) %> ago)
        </td>
      </tr>
    </table>
  </div>
</div>
