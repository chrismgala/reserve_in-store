<% if @current_store.users.any? %>
  <script>
      <% if @current_store.reservations.count > 0 %>
          Turbolinks.visit("<%= reservations_path %>");
      <% else %>
          Turbolinks.visit("<%= stores_settings_path(view: 'settings') %>");
      <% end %>
  </script>
<% else %>
  <%= render partial: 'stores/settings/setup' %>
<% end %>
